{
  "defaultSeverity": "warning",
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "arrow-return-shorthand": true,
    "ban": [true, ["_", "extend"], ["_", "isNull"], ["_", "isDefined"]],
    "class-name": true,
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [true, "spaces", 4],
    "interface-name": false,
    "jsdoc-format": true,
    "label-position": true,
    "max-line-length": [true, 150],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "debug", "info", "time", "timeEnd", "trace", "log"],
    "no-construct": true,
    "no-debugger": true,
    "no-default-export": true,
    "no-duplicate-imports": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-string-literal": true,
    "no-trailing-whitespace": true,
    "no-unnecessary-callback-wrapper": true,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "no-var-keyword": true,
    "object-literal-shorthand": true,
    "one-line": [true, "check-open-brace", "check-catch", "check-else", "check-finally"],
    "prefer-object-spread": true,
    "quotemark": [true, "single", "jsx-single", "avoid-template", "avoid-escape"],
    "radix": true,
    "semicolon": [true, "never"],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "triple-equals": [true, "allow-null-check"],
    "typedef": [true, "call-signature", "parameter", "property-declaration"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace"
      },
      {
        "index-signature": "space"
      }
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords",
      "allow-pascal-case"
    ],
    "whitespace": [
      false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
